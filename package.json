{
  "name": "oceanic.js",
  "version": "0.0.9",
  "description": "A nodejs library for interfacing with Discord.",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "module": "esm.mjs",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "require": "./dist/lib/index.js",
      "import": "./esm.mjs"
    }
  },
  "scripts": {
    "lint": "eslint lib --ext .ts",
    "lint:fix": "eslint lib --ext .ts --fix",
    "prepare": "node build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OceanicJS/Oceanic.git"
  },
  "keywords": [
    "discord",
    "library",
    "discord-api",
    "api"
  ],
  "author": "Donovan Daniels <hewwo@yiff.rocks>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OceanicJS/Oceanic/issues"
  },
  "homepage": "https://github.com/OceanicJS/Oceanic#readme",
  "devDependencies": {
    "@types/node": "^16.11.56",
    "@types/pako": "^2.0.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-newlines": "^1.2.3",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@augu/collections": "^1.1.0",
    "undici": "^5.10.0",
    "ws": "^8.8.1"
  },
  "engines": {
    "node": ">=16.16.0"
  },
  "optionalDependencies": {
    "erlpack": "^0.1.4",
    "pako": "^2.0.4",
    "zlib-sync": "^0.1.7"
  }
}
